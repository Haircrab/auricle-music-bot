{
	"extends": "@sapphire/ts-config",
	"compilerOptions": {
		"experimentalDecorators": true,
		"rootDir": "src",
		"outDir": "dist",
		"tsBuildInfoFile": "dist/.tsbuildinfo",
		"skipLibCheck": true,
		"noImplicitAny": false,
		"noImplicitReturns": false,
		"ignoreDeprecations": "5.0"
	},
	"include": ["src"]
}
